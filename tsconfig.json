{
  "include": [
    "src/**/*",
  ],
  "compilerOptions": {
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "esnext",
    "rootDir": "src/",
    "outDir": "dist/",
    "sourceMap": false,
    "strict": true,
    "typeRoots": [
      "bitburner/ScriptEditor"
    ]
  }
}
